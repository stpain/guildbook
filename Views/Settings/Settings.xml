                        </Anchors>
                        <Layers>
                            <Layer level="OVERLAY">
                                <FontString parentKey="header" inherits="GameFontNormalLarge" text="Guild Bank">
                                    <Size y="30"/>
                                    <Anchors>
                                        <Anchor point="TOPLEFT" x="0" y="0"/>
