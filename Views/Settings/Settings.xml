                                        <Anchor point="TOPLEFT" x="0" y="0"/>
                                        <Anchor point="TOPRIGHT" x="0" y="0"/>
                                    </Anchors>
                                </FontString>
                                <FontString parentKey="general" inherits="GameFontNormal" text="Addon">
                                    <Color r="1" g="1" b="1"/>
                                    <Size y="30"/>
                                    <Anchors>
                                        <Anchor point="TOPLEFT" x="0" y="-30"/>
                                        <Anchor point="TOPRIGHT" x="0" y="-30"/>
                                    </Anchors>
