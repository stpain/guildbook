                                </Anchors>
                                <Layers>
                                    <Layer level="OVERLAY">
                                        <FontString parentKey="general" inherits="GameFontNormalSmall" text="Discord" justifyH="LEFT">
                                            <Size x="200" y="20"/>
                                            <Anchors>
                                                <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" x="0" y="0"/>
                                            </Anchors>
                                        </FontString>
                                    </Layer>
                                </Layers>
                            </EditBox>

