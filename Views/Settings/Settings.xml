                                    <Anchors>
                                        <Anchor point="TOPLEFT" x="0" y="-30"/>
                                        <Anchor point="TOPRIGHT" x="0" y="-30"/>
                                    </Anchors>
                                </FontString>
                            </Layer>
                        </Layers>
                        <Frames>
                            <Slider name="GuildbookSettingsChatGuildHistoryLimitSlider" parentKey="chatGuildHistoryLimit" inherits = "GuildbookWrathEraSliderTopLabel">
                                <Anchors>
