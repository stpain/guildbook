                                <Anchors>
                                    <Anchor point="TOPLEFT" x="16" y="-70"/>
                                </Anchors>
                            </Button>

                            <CheckButton parentKey="debug" inherits = "GuildbookWrathEraWidgetsCheckButton">
                                <Size x="24" y="24"/>
                                <Anchors>
                                    <Anchor point="TOPLEFT" x="16" y="-105"/>
                                </Anchors>
                            </CheckButton>

