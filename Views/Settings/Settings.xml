                            <Anchor point="BOTTOMRIGHT" x="0" y="0"/>
                        </Anchors>
                        <Layers>
                            <Layer level="OVERLAY">
                                <FontString parentKey="header" inherits="GameFontNormalLarge" text="Addon">
